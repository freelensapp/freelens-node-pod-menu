    "eslint": "^8.31.0",
    "ts-loader": "^9.4.2",
    "webpack-cli": "^5.0.1"
  },
  "license": "MIT",
  "keywords": [
    "node",
    "pod",
    "menu",
    "extension",
    "k8slens",
    "lens",
    "openlens"
