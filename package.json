{
  "name": "@freelensapp/freelens-node-pod-menu",
  "version": "1.0.0",
  "description": "Node and pod menus for FreeLens",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/freelensapp/freelens-node-pod-menu.git",
    "directory": "."
  },
  "renderer": "dist/renderer.js",
  "engines": {
    "node": "^16.14.2",
